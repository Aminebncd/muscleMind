{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}

    
        {{user}} <br>
        {{user.email}} <br>
        {{user.score}} <br>

        {% if user.performances %} 
        {% for performance in user.performances %}
            {{performance}} <br>
        {% endfor %}
        {% endif %}

        {% if user.programs %} 
        {% for program in user.programs %}
            {{program}} <br>
        {% endfor %}
        {% endif %}

        {% if user.sessions %}
        {% for session in user.sessions %}
            {{session}} <br>
        {% endfor %}
        {% endif %}

        
        




{% endblock %}
